    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".producto_activity" />
        <activity android:name=".modelo.altaproducto" />
        <activity android:name=".altaproducto" />
        <activity android:name=".modelo.PedidoActivity" />
        <activity android:name=".modelo.HistorialProductos"></activity>

        <receiver android:name=".MyReceiver" android:exported="true">
            <intent-filter>
                <action android:name="<<paquete>>>.ESTADO_ACEPTADO"/>
                <action android:name="<<paquete>>>.ESTADO_CANCELADO"/>
                <action android:name="<<paquete>>>.ESTADO_EN_PREPARACION"/>
                <action android:name="<<paquete>>>.ESTADO_LISTO"/>
            </intent-filter>


        </receiver>



    </application>

</manifest>