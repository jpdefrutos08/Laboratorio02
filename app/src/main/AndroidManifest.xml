<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.edu.utn.frsf.dam.isi.laboratorio02">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".producto_activity" />
        <activity android:name=".modelo.altaproducto" />
        <activity android:name=".altaproducto" />
        <activity android:name=".modelo.PedidoActivity" />
        <activity android:name=".modelo.HistorialProductos" />

        <service
            android:name=".RestoFirebaseInstanceIdService"
            android:enabled="true"
            android:exported="true">
        </service>


        <service
            android:name=".RestoMessagingService"
            android:enabled="true"
            android:exported="true">
        </service>

        <service android:name=".RestoMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name=".RestoFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>



    </application>

</manifest>